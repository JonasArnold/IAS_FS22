    <pous>
      <pou name="FB_PID_Controller" pouType="functionBlock">
        <interface>
          <inputVars>
            <variable name="ref">
              <type>
                <REAL />
              </type>
              <documentation>
                <xhtml xmlns="http://www.w3.org/1999/xhtml"> Aktuelle Referenzgroesse (Soll-Groesse)</xhtml>
