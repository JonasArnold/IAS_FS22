            </variable>
            <variable name="y">
              <type>
                <REAL />
              </type>
              <documentation>
                <xhtml xmlns="http://www.w3.org/1999/xhtml"> Aktuelle Regelgroesse (Ist-Groesse)</xhtml>
              </documentation>
            </variable>
            <variable name="Mode">
              <type>
                <derived name="E_System_Mode" />
              </type>
              <documentation>
